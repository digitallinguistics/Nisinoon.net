@article{wolvengrey_prospective_2006,
	title = {Prospective {Aspect} in the {Western} {Dialects} of {Cree}},
	volume = {72},
	issn = {00207071},
	url = {https://drive.google.com/file/d/1Y4FiFrI2h7FGNnraD58YatTtarFCIVpM/view?usp=sharing},
	doi = {10.1086/509491},
	abstract = {This paper seeks to clarify the difference between two markers of future time reference in the Cree language. The preverbal particles ta- (∼ ka-) and wî- have both generally been treated as future tense markers. While future tense is found to be sufficient characterization for ta-, wî- requires further consideration, resulting in the conclusion that, rather than simple tense, it marks prospective aspect. The characterization of wî- in this way ties together a number of alternate interpretations as secondary semantic and/or pragmatic offshoots of the basic category of the prospective. Though data are presented in the Plains Cree dialect, native-speaker judgments confirm the arguments for all the major western dialects, Plains, Woods, and Swampy Cree, as spoken in Saskatchewan and Manitoba. © 2006 by The University of Chicago. All rights reserved.},
	number = {3},
	journal = {International Journal of American Linguistics},
	author = {Wolvengrey, Arok},
	year = {2006},
	keywords = {Algonquian languages, Aspect, Cree language, Prospective, Tense},
	pages = {397--407},
}

